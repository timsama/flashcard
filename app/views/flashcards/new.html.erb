<h1>Add a Flashcard</h1>
<%= form_for @new_flashcard, :url=>{:action=>"create"} do |f| %>
  <%= f.label :keyword %>
  <%= f.text_field :keyword %>
<br>
  <%= f.label :kanji %>
  <%= f.text_field :kanji %>
<br>
  <%= f.submit "Create Flashcard", class: "btn btn-primary" %>
  <%= button_to "Cancel", flashcards, :method => :get %>
<% end %>
