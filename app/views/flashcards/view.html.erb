<h1>View Flashcards</h1>
<ul>
<% for flashcard in @flashcards %>
  <li><%= flashcard.keyword %></li>
<% end %>
</ul>
<h1>Add a Flashcard</h1>
<%= form_for @new_flashcard, :url=>{:action=>"create"} do |f| %>
  Keyword <%= f.text_field :keyword %><br>
  Kanji <%= f.text_field :kanji %><br>
  <%= f.submit "Create Flashcard", class: "btn btn-primary" %>
<% end %>
